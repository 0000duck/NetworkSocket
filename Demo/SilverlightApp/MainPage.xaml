<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk" x:Class="SilverlightApp.MainPage"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <Grid x:Name="LayoutRoot" Background="White">
        <sdk:Label HorizontalAlignment="Left" Height="20" Margin="61,44,0,0" VerticalAlignment="Top" Width="33" Content="账号"/>
        <sdk:Label HorizontalAlignment="Left" Height="20" Margin="61,87,0,0" VerticalAlignment="Top" Width="33" Content="密码"/>
        <sdk:Label Name="LabelMsg" HorizontalAlignment="Left" Height="20" Margin="110,116,0,0" VerticalAlignment="Top" Width="153" Content="" RenderTransformOrigin="5.242,2.55"/>
        <TextBox Name="TextAccount" HorizontalAlignment="Left" Height="24" Margin="110,40,0,0" TextWrapping="Wrap" Text="admin" VerticalAlignment="Top" Width="153"/>
        <TextBox Name="TextPassword" HorizontalAlignment="Left" Height="24" Margin="110,83,0,0" TextWrapping="Wrap" Text="123456" VerticalAlignment="Top" Width="153"/>
        <Button Name="BtnLogin" Content="登录" HorizontalAlignment="Left" Height="24" Margin="180,140,0,0" VerticalAlignment="Top" Width="83" Click="BtnLogin_Click"/>
    </Grid>
</UserControl>
